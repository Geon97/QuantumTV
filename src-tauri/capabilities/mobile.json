{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Capability for mobile platforms (Android/iOS)",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "*" }],
      "deny": []
    }
  ],
  "platforms": ["android", "iOS"]
}
